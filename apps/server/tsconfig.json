{
  "include": ["./src/**/*"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
